<table class="table table-bordered">
  <thead class = "thead-dark">
    <tr>
      <th scope="col">#</th>
      <th scope="col">Username</th>
      <th scope="col">Password</th>
      <th scope="col">Email</th>
      <th scope="col">Age</th>
      <th scope="col" class = "text-end">Action</th>
    </tr>
  </thead>
  <tbody>

    {{#each rows}}




    <tr>
      <th scope="row">{{this.userid}}</th>
      <td>{{this.username}}</td>
      <td>{{this.password}}</td>
      <td>{{this.email}}</td>
      <td>{{this.age}}</td>
      <td class = "text-end">

        <a href = "#" type = "button" class = "btn btn-light btn-small"><i class = "bi bi-eye"></i> View</a>
        <a href = "#" type = "button" class = "btn btn-light btn-small"><i class = "bi bi-pencil"></i> Edit</a>
        <a href = "#" type = "button" class = "btn btn-light btn-small"><i class = "bi bi-person-x"></i> Delete</a>

      </td>
    </tr>

    {{/each}}
  </tbody>
</table>